<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Admin/images/favicon.ico">

    <!-- App css -->
    <link href="~/Admin/css/config/material/bootstrap-rtl.min.css" rel="stylesheet" type="text/css" id="bs-default-stylesheet" />
    <link href="~/Admin/css/config/material/app-rtl.min.css" rel="stylesheet" type="text/css" id="app-default-stylesheet" />

    <link href="~/Admin/css/config/material/bootstrap-dark-rtl.min.css" rel="stylesheet" type="text/css" id="bs-dark-stylesheet" />
    <link href="~/Admin/css/config/material/app-dark-rtl.min.css" rel="stylesheet" type="text/css" id="app-dark-stylesheet" />

    <!-- icons -->
    <link href="~/Admin/css/icons-rtl.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Admin/css/fontiran.css" rel="stylesheet" type="text/css" />
    
    <link href="~/Admin/sweetalert/sweetalert.css" rel="stylesheet" type="text/css" />
    <link type="text/css" rel="stylesheet" href="/Admin/JalaliDatePicker/css/jalalidatepicker.min.css" />
    @RenderSection("Styles", required: false)

</head>
<body class="loading" data-layout='{"mode": "light", "width": "fluid", "menuPosition": "fixed", "sidebar": { "color": "light", "size": "default", "showuser": false}, "topbar": {"color": "dark"}, "showRightSidebarOnPageLoad": true}'>

    <!-- Begin page -->
    <div id="wrapper">
        <partial name="../../Views/Partial/_TopBar.cshtml" />
        <partial name="../../Views/Partial/_LeftSideBar.cshtml" />

        <div class="content-page">
            <div class="content">
                @RenderBody()
            </div>
            <partial name="../../Views/Partial/_Footer.cshtml" />
        </div>
    </div>
    @*<partial name="../../Views/Partial/_RightSideBar.cshtml" />*@
    
    <!-- Right bar overlay-->
    @*<div class="rightbar-overlay"></div>*@

    <!-- Vendor js -->
    <script src="~/Admin/js/vendor.min.js"></script>
     @RenderSection("SearchScripts", required: false)
    <script src="~/Admin/sweetalert/sweetalert.min.js"></script>
    
    <script src="~/Admin/persiandate/persian-date.js"></script>
    <script src="~/Admin/MyJs.js"></script>
    <script src="~/Admin/Report.js"></script>
      <script type="text/javascript" src="/Admin/JalaliDatePicker/js/jalalidatepicker.min.js"></script>
   <script>
            jalaliDatepicker.startWatch({
                minDate: "attr",
                maxDate: "attr",
                time:true
            }); 
    </script>
     @RenderSection("Scripts", required: false)
    <!-- App js-->
    <script src="~/Admin/js/app.min.js"></script>

    @*<script src="~/Admin/Admin.js" asp-append-version="true"></script>*@
</body>
</html>
